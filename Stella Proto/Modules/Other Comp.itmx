                <from>Inventory_Ratio</from>
                <to>Supply</to>
            </connector>
            <connector x="251" y="155" uid="43" color="#FF007F">
                <from>Effect_On_Price</from>
                <to>Desired_Price</to>
            </connector>
            <connector x="211" y="241" uid="44" color="#FF007F">
                <from>Change_Delay</from>
                <to>Change_in_Price</to>
            </connector>
            <connector x="168" y="400" uid="45" color="#FF007F">
                <from>Supply_Curve</from>
                <to>Supply</to>
            </connector>
            <connector x="433" y="359" uid="46" color="#FF007F">
                <from>Total_Market</from>
                <to>Supply_Curve</to>
            </connector>
            <connector x="439" y="65" uid="47" color="#FF007F">
                <from>Demand_Curve</from>
                <to>Inventory_Rat</to>
            </connector>
            <connector x="175" y="466" uid="48" color="#FF007F">
                <from>Production_Price</from>
                <to>Supply_Curve</to>
            </connector>
            <connector x="327" y="298" uid="49" color="#FF007F">
                <from>Price</from>
                <to>Supply_Curve</to>
            </connector>
            <connector x="360" y="265" uid="50" color="#FF007F">
                <from>Price</from>
                <to>Demand_Curve</to>
            </connector>
            <connector x="274" y="240" uid="51" color="#FF007F">
                <from>Desired_Price</from>
                <to>Change_in_Price</to>
            </connector>
            <connector x="151" y="71" uid="52" color="#FF007F">
                <from>Inventory_Ratio</from>
                <to>Current_Stock</to>
            </connector>
            <connector x="202" y="45" uid="53" color="#FF007F">
                <from>Current_Stock</from>
