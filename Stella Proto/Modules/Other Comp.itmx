            </connector>
            <connector x="316" y="294" uid="54" color="#FF007F">
                <from>Price</from>
                <to>Change_in_Price</to>
            </connector>
            <connector x="326" y="264" uid="55" color="#FF007F">
                <from>Price</from>
                <to>Desired_Price</to>
            </connector>
        </display>
