            </connector>
            <connector x="444" y="344" uid="39" color="#FF007F">
                <from>Total_Market</from>
                <to>Demand_Curve</to>
            </connector>
            <connector x="251" y="45" uid="40" color="#FF007F">
                <from>Inventory_Rat</from>
                <to>Effect_On_Price</to>
            </connector>
            <connector x="70" y="176" uid="41" color="#FF007F">
                <from>Supply</from>
                <to>Profit</to>
            </connector>
